def primeSieve(n):
    prime = [True for i in range(n+1)]
    currentNumber=2
    while(currentNumber**2 <=n):
        if prime[currentNumber] == True:
            for i in range(currentNumber ** 2, n + 1, currentNumber):
                prime[i] = False
        currentNumber += 1
    for p in range(n+1):
        if prime[p]:
            print(p)
        
n = 99

primeSieve(n)
