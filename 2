num = int(input("Enter a number:"))

a = num
reversed_n = 0

while num > 0:
    digit = num%10
    reversed_n = reversed_n * 10 + digit
    num //= 10

if a == reversed_n:
    print("{} is a pallindrome".format(a))
else:
    print("{} is not a pallindrome".format(a))

num1 = int(input("Enter the largest number"))
num2 = int(input("Enter the smallest number:"))

while num2:
    numStore = num2
    num2 = num1%num2
    num1 = numStore

print("HCF is {}".format(num1))
